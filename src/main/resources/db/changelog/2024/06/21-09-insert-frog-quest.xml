<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <changeSet id="9-32-add-quest" author="alimova">
        <insert tableName="quest">
            <column name="id">2</column>
            <column name="title">Сад лягушек</column>
            <column name="description">Вроде до этого ты ложился в своей кровати и сладко засыпал. Однако сейчас над тобой висит лицо лягушки, которая заговорила с тобой...</column>
        </insert>

    </changeSet>

    <changeSet id="9-36-add-point-4" author="alimova">
        <insert tableName="point">
            <column name="id">4</column>
            <column name="question">Над тобой лягушка, которая говорит: 'Ты добрый воин, который должен пройти посвящение. Впереди нас ждёт короткий путь, чтобы стать избранным'</column>
            <column name="quest_id">2</column>
        </insert>

        <insert tableName="action">
            <column name="id">7</column>
            <column name="status_point">WIN</column>
            <column name="point_id">4</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">11</column>
            <column name="serial_number">1</column>
            <column name="description">Посчитать, что это бред</column>
            <column name="action_id">7</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">12</column>
            <column name="serial_number">2</column>
            <column name="description">Ущепнуть себя</column>
            <column name="action_id">7</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">13</column>
            <column name="serial_number">3</column>
            <column name="description">Ты проснулся</column>
            <column name="action_id">7</column>
        </insert>

        <insert tableName="action">
            <column name="id">8</column>
            <column name="status_point">ACTIVE</column>
            <column name="point_id">4</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">14</column>
            <column name="serial_number">1</column>
            <column name="description">Поздороваться с лягушкой</column>
            <column name="action_id">8</column>
        </insert>
        <insert tableName="step_action">
            <column name="id">15</column>
            <column name="serial_number">2</column>
            <column name="description">Поприветствовать её</column>
            <column name="action_id">8</column>
        </insert>
    </changeSet>

    <changeSet id="17-09-add-first-point-quest" author="alimova">
        <insert tableName="quest_first_point">
            <column name="id">2</column>
            <column name="quest_id">2</column>
            <column name="point_id">4</column>
        </insert>
    </changeSet>

    <changeSet id="17-09-add-point-5" author="alimova">
        <insert tableName="point">
            <column name="id">5</column>
            <column name="question">Лягушка тянет тебя за рукав. Впереди тебя пахучий сад и тонкая тропинка. Ты послушно идёшь за лягушкой</column>
            <column name="quest_id">2</column>
        </insert>

        <insert tableName="action">
            <column name="id">9</column>
            <column name="status_point">WIN</column>
            <column name="point_id">5</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">16</column>
            <column name="serial_number">1</column>
            <column name="description">Насторожиться, оглянуться вокруг</column>
            <column name="action_id">9</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">17</column>
            <column name="serial_number">2</column>
            <column name="description">Ты замечаешь узкую дверь прямо в воздухе позади себя</column>
            <column name="action_id">9</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">18</column>
            <column name="serial_number">3</column>
            <column name="description">Ты сбегаешь из сна и просыпаешься в недоумении</column>
            <column name="action_id">9</column>
        </insert>

        <insert tableName="action">
            <column name="id">10</column>
            <column name="status_point">ACTIVE</column>
            <column name="point_id">5</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">19</column>
            <column name="serial_number">1</column>
            <column name="description">Пойти за лягушкой по тропинке</column>
            <column name="action_id">10</column>
        </insert>
    </changeSet>

    <changeSet id="17-15-add-next-point-to-action-8" author="alimova">
        <insert tableName="point_action">
            <column name="id">3</column>
            <column name="action_id">8</column>
            <column name="next_point_id">5</column>
        </insert>
    </changeSet>

    <changeSet id="17-16-add-point-6" author="alimova">
        <insert tableName="point">
            <column name="id">6</column>
            <column name="question">Лягушка начинает рассказывать про просвящение. Один из шагов - съесть протянутого жука.</column>
            <column name="quest_id">2</column>
        </insert>

        <insert tableName="action">
            <column name="id">11</column>
            <column name="status_point">LOSS</column>
            <column name="point_id">6</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">20</column>
            <column name="serial_number">1</column>
            <column name="description">Жук шевелится в лапке лягушки. Почувствовать отвращение</column>
            <column name="action_id">11</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">21</column>
            <column name="serial_number">2</column>
            <column name="description">Ты отказываешься. Лягушка начинает злиться на то, что ты не принял её угощение</column>
            <column name="action_id">11</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">22</column>
            <column name="serial_number">2</column>
            <column name="description">Лягушка увеличивается в размерах. Теперь она глотает вас как жука</column>
            <column name="action_id">11</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">23</column>
            <column name="serial_number">3</column>
            <column name="description">Вы просыпаетесь в холодном поту, чувствуя боль во всём теле. Однако теперь у вас нет ног под одеялом...</column>
            <column name="action_id">11</column>
        </insert>

        <insert tableName="action">
            <column name="id">12</column>
            <column name="status_point">ACTIVE</column>
            <column name="point_id">6</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">24</column>
            <column name="serial_number">1</column>
            <column name="description">Во сне всё нереально. Жук вряд ли будет иметь какой-то вкус.</column>
            <column name="action_id">12</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">25</column>
            <column name="serial_number">2</column>
            <column name="description">Согласиться на угощение. Съесть жука под довольное кваканье жабы.</column>
            <column name="action_id">12</column>
        </insert>

    </changeSet>

    <changeSet id="17-20-add-next-point-to-action-10" author="alimova">
        <insert tableName="point_action">
            <column name="id">4</column>
            <column name="action_id">10</column>
            <column name="next_point_id">6</column>
        </insert>
    </changeSet>

    <changeSet id="17-21-add-point-7" author="alimova">
        <insert tableName="point">
            <column name="id">7</column>
            <column name="question">Вокруг поют птицы и жужжат насекомые. После серых будней нахождение в саду с разговаривающей лягушкой
                кажется чем-то уникальным. Лягушка ненавязчиво говорит, что их жизнь лишена волнения и страха</column>
            <column name="quest_id">2</column>
        </insert>

        <insert tableName="action">
            <column name="id">13</column>
            <column name="status_point">WIN</column>
            <column name="point_id">7</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">26</column>
            <column name="serial_number">1</column>
            <column name="description">Невольно согласиться с лягушкой</column>
            <column name="action_id">13</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">27</column>
            <column name="serial_number">2</column>
            <column name="description">Ты давно не бывал в просторах успокаивающей природы. Здесь тебе хорошо и не хочется уходить</column>
            <column name="action_id">13</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">28</column>
            <column name="serial_number">3</column>
            <column name="description">Ты чувствуешь, как твои руки уменьшаются, а между пальцами появляются перепонки</column>
            <column name="action_id">13</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">29</column>
            <column name="serial_number">4</column>
            <column name="description">Всё не важно, ведь теперь ты просто лягушка, наслаждающаяся жизнью под солнцем на кувшинке</column>
            <column name="action_id">13</column>
        </insert>

        <insert tableName="action">
            <column name="id">14</column>
            <column name="status_point">LOSS</column>
            <column name="point_id">7</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">30</column>
            <column name="serial_number">1</column>
            <column name="description">Как бы здесь не было хорошо, ты не хочешь быть лягушкой</column>
            <column name="action_id">14</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">31</column>
            <column name="serial_number">2</column>
            <column name="description">Это злит лягушку. Она толкает тебя в пруд, где за твои ноги хватаются чужие лапы. Они утаскивают тебя под воду</column>
            <column name="action_id">14</column>
        </insert>

        <insert tableName="step_action">
            <column name="id">32</column>
            <column name="serial_number">3</column>
            <column name="description">Ты просыпаешься в ванной, чуть ли не захлебнувшись водой.</column>
            <column name="action_id">14</column>
        </insert>


    </changeSet>

    <changeSet id="17-26-add-next-point-to-action-12" author="alimova">
        <insert tableName="point_action">
            <column name="id">5</column>
            <column name="action_id">12</column>
            <column name="next_point_id">7</column>
        </insert>
    </changeSet>

</databaseChangeLog>